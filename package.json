{
  "name": "kaioken-csr-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx ./server.ts --watch",
    "build": "pnpm build:client && pnpm build:server && pnpm build:ssg",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:ssg": "cross-env NODE_ENV=production node ./docs-ssg.js",
    "preview": "cross-env NODE_ENV=production tsx ./server.ts"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.18",
    "compression": "^1.8.0",
    "sirv": "^3.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@tailwindcss/postcss": "^4.1.6",
    "@types/mdx": "^2.0.13",
    "cross-env": "^7.0.3",
    "express": "^5.1.0",
    "kaioken": "^0.39.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.6",
    "vite": "^6.3.5",
    "vite-plugin-kaioken": "^0.19.0"
  }
}
